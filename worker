	<FONT FACE="System"><FONT SIZE=2><B>			var salt =
	'KoNXm0UsxTkY2Xof';</B></FONT></FONT><DT>
	<FONT FACE="System"><FONT SIZE=2><B>			var pw_realization_strategy =
	'sha';</B></FONT></FONT><DT>
